<section class="container-fluid cpcontainer">
<%= render partial: "cp/cp" %>
	<section class="col-sm-9 pagecontainer ">
	<table class="table table-striped table-bordered table-hover table-condensed table-responsive">
			<tr>
		    	<th>id</th>
		    	<th>email</th>
		    	<th>provider</th>
		    	<th>joined from</th>
			</tr>
		<% @users.each do |u| %>
			<tr>
				<th><%=u.id%></th>
				<th><%=u.email%></th>
				<th><%if u.provider.present?%><%=u.provider%><%else%>Manual signup<%end%></th>
				<th><%=u.created_at%></th>
			</tr>
		<% end %>
	</table>
	</section>
</section>
<script>
		$(document).ready(function(){
			$('#newstab').attr('class', 'list-group-item');
			$('#userstab').attr('class', 'list-group-item active');
		})
	</script>