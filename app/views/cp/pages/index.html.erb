<section class="container-fluid cpcontainer">
<%= render partial: "cp/cp" %>
	<section class="col-sm-9 pagecontainer ">
	<table class="table table-striped table-bordered table-hover table-condensed table-responsive">
			<tr>
		    	<th>id</th>
		    	<th>title</th>
		    	<th>Upper text</th>
		    	<th>Lower text</th>
		    	<th>Edit</th>
			</tr>
		<% @pages.order("created_at ASC").each do |s| %>
			<tr>
				<th><%=s.id%></th>
				<th><%=s.head%></th>
				<th><%=s.text1%></th>
				<th><%=s.text2%></th>
				<th><%= link_to "edit", edit_cp_page_path(s) %></th>
			</tr>
		<% end %>
	</table>
	</section>
</section>
<script>
		$(document).ready(function(){
			$('#newstab').attr('class', 'list-group-item');
			$('#pagestab').attr('class', 'list-group-item active');
		})
	</script>